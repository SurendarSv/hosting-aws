sudo -i
sudo apt update
yum install httpd   ///static web host-->apache (to host the apache)
                    /// apache installed ///
service httpd start 
cd/var/www/html/ 
pwd                 /// open file
cd..
cd.
cd                  /// open cd file
pwd                 
cd/var/
ls                  // list file
cd/html/
ls
pwd                 // opens html file use WINSCP to transfer the HTML CODE to the linux instances(machine)
sudo mv * /var/ww/html/
logout
sudo -i
